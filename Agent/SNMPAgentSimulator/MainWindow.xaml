<Window x:Class="SNMPAgentSimulator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SNMPAgentSimulator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="415.522">
    <Grid Margin="0,0,-8,0">
        <Button Content="Send Trap" HorizontalAlignment="Left" Margin="10,329,0,0" VerticalAlignment="Top" Width="385" Height="61" FontSize="18" Click="SendTrap"/>
        <TextBox x:Name="txtBoxManagerIP" HorizontalAlignment="Left" Height="23" Margin="100,13,0,0" TextWrapping="Wrap" Text="{Binding ip, Mode=TwoWay}" VerticalAlignment="Top" Width="200" />
        <TextBox HorizontalAlignment="Left" Height="23" Margin="100,44,0,0" TextWrapping="Wrap" Text="{Binding port, Mode=TwoWay}" VerticalAlignment="Top" Width="200"/>
        <Label Content="Manager IP" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="85"/>
        <Label Content="Manager Port" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="85"/>

    </Grid>
</Window>
