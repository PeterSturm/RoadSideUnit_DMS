@model IEnumerable<DashboardWebApp.Models.RSU>

<table width="100%" class="table table-borderless table-sm">
    <thead class="thead-dark">
        <tr>
            <th>Name</th>
            <th>Manager</th>
            <th>Position</th>
        </tr>    
    </thead>
    <tbody>
        @foreach (var rsu in Model)
        {
        <tr>
            <td>@rsu.Name</td>
            <td>@rsu.Manager.Name</td>
            <td>@rsu.Latitude @rsu.Longitude</td>
        </tr>
        }
    </tbody>
</table>